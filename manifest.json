{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Traduce"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "interact.js"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Catify your websites",
  "manifest_version": 2,
  "name": "Catify",
  "permissions": [
    "activeTab"
  ],
  "version": "2.0"
}
